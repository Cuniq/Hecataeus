SELECT title, fid FROM {aggregator_feed} ORDER BY title;
SELECT title, cid FROM {aggregator_category} ORDER BY title;
SELECT * FROM {aggregator_feed} WHERE checked + refresh < %d;
SELECT cid, title FROM {aggregator_category} ORDER BY title;
SELECT fid, title FROM {aggregator_feed} ORDER BY fid;
SELECT block FROM {aggregator_category} WHERE cid = %d;
SELECT block FROM {aggregator_feed} WHERE fid = %d;
SELECT fid, title, block FROM {aggregator_feed} WHERE fid = %d;
SELECT * FROM {aggregator_item} WHERE fid = %d ORDER BY timestamp DESC, iid DESC;
SELECT cid, title, block FROM {aggregator_category} WHERE cid = %d;
SELECT i.* FROM {aggregator_category_item} ci LEFT JOIN {aggregator_item} i ON ci.iid = i.iid WHERE ci.cid = %d ORDER BY i.timestamp DESC, i.iid DESC;
SELECT iid FROM {aggregator_item} WHERE fid = %d;
SELECT iid FROM {aggregator_item} WHERE fid = %d AND link = '%s';
SELECT iid FROM {aggregator_item} WHERE fid = %d AND title = '%s';
SELECT iid FROM {aggregator_item} WHERE fid = %d AND timestamp < %d;
SELECT cid FROM {aggregator_category_feed} WHERE fid = %d;
SELECT c.cid, c.title, f.fid FROM {aggregator_category} c LEFT JOIN {aggregator_category_feed} f ON c.cid = f.cid AND f.fid = %d ORDER BY title;
SELECT iid FROM {aggregator_item} WHERE fid = %d;
SELECT * FROM {aggregator_feed} WHERE fid = %d;
SELECT * FROM {aggregator_category} WHERE cid = %d;
SELECT f.*, COUNT(i.iid) AS items FROM {aggregator_feed} f LEFT JOIN {aggregator_item} i ON f.fid = i.fid GROUP BY f.fid, f.title, f.url, f.refresh, f.checked, f.link, f.description, f.etag, f.modified, f.image, f.block ORDER BY f.title;
SELECT c.cid, c.title, count(ci.iid) as items FROM {aggregator_category} c LEFT JOIN {aggregator_category_item} ci ON c.cid = ci.cid GROUP BY c.cid, c.title ORDER BY title;
SELECT i.*, f.title AS ftitle, f.link AS flink FROM {aggregator_item} i INNER JOIN {aggregator_feed} f ON i.fid = f.fid ORDER BY i.timestamp DESC, i.iid DESC;
SELECT * FROM {aggregator_feed} WHERE fid = %d;
SELECT cid, title FROM {aggregator_category} WHERE cid = %d;
SELECT i.*, f.title AS ftitle, f.link AS flink FROM {aggregator_category_item} c LEFT JOIN {aggregator_item} i ON c.iid = i.iid LEFT JOIN {aggregator_feed} f ON i.fid = f.fid WHERE cid = '. $category->cid .' ORDER BY timestamp DESC, iid DESC;
SELECT c.cid, c.title, ci.iid FROM {aggregator_category} c LEFT JOIN {aggregator_category_item} ci ON c.cid = ci.cid AND ci.iid = %d;
SELECT f.fid, f.title, f.description, f.image, MAX(i.timestamp) AS last FROM {aggregator_feed} f LEFT JOIN {aggregator_item} i ON f.fid = i.fid GROUP BY f.fid, f.title, f.description, f.image;
SELECT i.title, i.timestamp, i.link FROM {aggregator_item} i WHERE i.fid = %d ORDER BY i.timestamp DESC;
SELECT * FROM {aggregator_feed} ORDER BY title;
SELECT c.cid, c.title, c.description FROM {aggregator_category} c LEFT JOIN {aggregator_category_item} ci ON c.cid = ci.cid LEFT JOIN {aggregator_item} i ON ci.iid = i.iid GROUP BY c.cid, c.title, c.description;
SELECT i.title, i.timestamp, i.link, f.title as feed_title, f.link as feed_link FROM {aggregator_category_item} ci LEFT JOIN {aggregator_item} i ON i.iid = ci.iid LEFT JOIN {aggregator_feed} f ON i.fid = f.fid WHERE ci.cid = %d ORDER BY i.timestamp DESC
SELECT c.title, c.cid FROM {aggregator_category_item} ci LEFT JOIN {aggregator_category} c ON ci.cid = c.cid WHERE ci.iid = %d ORDER BY c.title;
